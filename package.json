{
  "name": "mwf-react-testing",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"json-server --watch db.json --port 3001 --m ./node_modules/json-server-auth\" \"vite --open\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "cypress:open": "concurrently \"json-server --watch db.json --port 3001 --m ./node_modules/json-server-auth\" \"cypress open\"",
    "e2e:open": "concurrently \"npm run dev\" \"npx cypress open --e2e\""
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.0",
    "axios": "^1.7.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.1.1",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@redux-devtools/extension": "^3.3.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/vite": "^4.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "cypress": "^14.0.1",
    "cypress-localstorage-commands": "^2.2.7",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.13.0",
    "jsdom": "^26.0.0",
    "json-server": "^0.17.4",
    "json-server-auth": "^2.1.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^4.0.3",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.18.1",
    "vite": "^6.0.3",
    "vitest": "^3.0.5"
  }
}
